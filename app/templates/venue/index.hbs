<h4>Tilasi</h4>
<ul class="no-bullet">
  {{#each model}}
    {{#if this.id}}
      <li>{{#link-to 'venue.edit' this}}{{this.title}}{{/link-to}}</li>
    {{else}}
      <li class="disabled">{{this.title}}</li>
    {{/if}}
  {{/each}}
</ul>
{{#link-to 'venue.wizard' id='create' class='button success'}}
  Luo uusi tila
{{/link-to}}


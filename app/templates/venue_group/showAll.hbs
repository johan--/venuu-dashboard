<h4>Kohteet</h4>
  <ul class="no-bullet">
    {{#each this}}
      {{#if this.id}}
        <li>{{#link-to 'venue_group.edit' this}}{{this.name}}{{/link-to}}</li>
      {{else}}
        <li class="disabled">{{this.name}}</li>
      {{/if}}
    {{/each}}
  </ul>
<button id="create" class="success" {{action 'create'}}>Luo uusi kohde</button>
